{
  "name": "rotatecube",
  "version": "1.0.0",
  "description": "rotating cubes constructor is a workout",
  "targets": {
    "main": false
  },
  "main": "dist/main.js",
  "source": "source-files/index.pug",
  "browserslist": "> 0.5%, last 2 versions, not dead",
  "scripts": {
    "sw": "start node ./node_modules/switching-environment-files/switching-environment-files.js",
    "lint": "eslint",
    "sassmin": "sass --watch ./source-files/perspectivo.scss:./source-files/perspectivo.css --no-source-map --style=compressed",
    "sass": "sass --watch ./source-files/perspectivo.scss:./source-files/perspectivo.css --no-source-map --update",
    "dev": "parcel --open --port 5000",
    "pro": "parcel build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/VladislavGolikov/CubeRotate.git"
  },
  "keywords": [
    "cube",
    "constructor"
  ],
  "author": "Vladislav Golikov",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/VladislavGolikov/CubeRotate/issues"
  },
  "homepage": "https://github.com/VladislavGolikov/CubeRotate#readme",
  "devDependencies": {
    "@parcel/transformer-inline-string": "^2.2.1",
    "@parcel/transformer-pug": "^2.2.1",
    "@parcel/transformer-sass": "^2.2.1",
    "dotenv": "^10.0.0",
    "eslint": "^8.5.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-security": "^1.4.0",
    "parcel": "^2.2.1",
    "switching-environment-files": "^1.0.11"
  },
  "dependencies": {
    "colors": "^1.4.0"
  }
}
